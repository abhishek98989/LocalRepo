{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "projectmanagement-overview-tool-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectmanagementOverviewTool/ProjectmanagementOverviewToolWebPart.js",
          "manifest": "./src/webparts/projectmanagementOverviewTool/ProjectmanagementOverviewToolWebPart.manifest.json"
        }
      ]
    },
    "task-notification-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskNotificationManagement/TaskNotificationManagementWebPart.js",
          "manifest": "./src/webparts/taskNotificationManagement/TaskNotificationManagementWebPart.manifest.json"
        }
      ]
    },
    "create-task-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createTask/CreateTaskWebPart.js",
          "manifest": "./src/webparts/createTask/CreateTaskWebPart.manifest.json"
        }
      ]
    },
    "project-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectManagement/ProjectManagementWebPart.js",
          "manifest": "./src/webparts/projectManagement/ProjectManagementWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "bootstrap-css": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css",
      "globalName": "Common-css"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js Jump ",
      "globalName": "bootstrap"
    },
    "bootstrap-js": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js",
      "globalName": "common-js"
    },
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "TaskNotificationManagementWebPartStrings": "lib/webparts/taskNotificationManagement/loc/{locale}.js",
    "CreateTaskWebPartStrings": "lib/webparts/createTask/loc/{locale}.js",
    "ProjectmanagementOverviewToolWebPartStrings": "lib/webparts/projectmanagementOverviewTool/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "ProjectManagementWebPartStrings": "lib/webparts/projectManagement/loc/{locale}.js"
  
  }
}
