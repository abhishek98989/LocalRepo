{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "taskprofile-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskprofile/TaskprofileWebPart.js",
          "manifest": "./src/webparts/taskprofile/TaskprofileWebPart.manifest.json"
        }
      ]
    },
    "service-portfolio-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/servicePortfolio/ServicePortfolioWebPart.js",
          "manifest": "./src/webparts/servicePortfolio/ServicePortfolioWebPart.manifest.json"
        }
      ]
    },
    "component-comments-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/componentComments/ComponentCommentsWebPart.js",
          "manifest": "./src/webparts/componentComments/ComponentCommentsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "bootstrap-css": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css",
      "globalName": "Common-css"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js Jump ",
      "globalName": "bootstrap"
    },
    "bootstrap-js": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js",
      "globalName": "common-js"
    },
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "ServicePortfolioWebPartStrings": "lib/webparts/servicePortfolio/loc/{locale}.js",
    "TaskprofileWebPartStrings": "lib/webparts/taskprofile/loc/{locale}.js",
    "ComponentCommentsWebPartStrings": "lib/webparts/componentComments/loc/{locale}.js"
  }
}